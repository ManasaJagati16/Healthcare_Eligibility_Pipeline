{
  "app_name": "Healthcare Eligibility Pipeline",
  "spark_conf": {
    "spark.sql.ansi.enabled": "false"
  },
  "partners": {
    "acme": {
      "file_path": "data/acme.txt",
      "delimiter": "|",
      "has_header": true,
      "partner_code": "ACME",
      "column_mapping": {
        "MBI": "external_id",
        "FNAME": "first_name",
        "LNAME": "last_name",
        "DOB": "dob",
        "EMAIL": "email",
        "PHONE ": "phone"
      },
      "dob_formats": ["MM/dd/yyyy"]
    },
    "bettercare": {
      "file_path": "data/bettercare.csv",
      "delimiter": ",",
      "has_header": true,
      "partner_code": "BETTERCARE",
      "column_mapping": {
        "subscriber_id": "external_id",
        "first_name": "first_name",
        "last_name": "last_name",
        "date_of_birth": "dob",
        "email": "email",
        "phone": "phone"
      },
      "dob_formats": ["yyyy-MM-dd"]
    }
  },
  "output": {
    "writer": "pandas",
    "path": "output/unified_eligibility.csv",
    "rejected_path": "output/rejected_rows.csv"
  }
}
